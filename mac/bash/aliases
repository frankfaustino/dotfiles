# MacVim
alias v='/Applications/MacVim.app/Contents/bin/mvim'

# Directory Stuff
alias ..="cd .."
alias cdd="cd -"
alias ...="cd ../.."
alias ls="ls -lG"
alias ll="ls -alh"
alias lsa="ls -lGa"

# Shortcuts
alias src="cd ~/src"
alias c="cd ~/Code"
alias d="cd ~/Desktop"
alias dl="cd ~/Downloads"
alias doc="cd ~/Documents"
alias dotfiles="cd ~/.dotfiles"
alias python='python3'
alias pip='pip3'
