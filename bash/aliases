# MacVim
alias v='/Applications/MacVim.app/Contents/bin/mvim'

# Directory Stuff
alias ..="cd .."
alias cdd="cd -"
alias ...="cd ../.."
alias ls="ls -lG"
alias ll="ls -alh"
alias lsa="ls -lGa"

# Shortcuts
alias docs="cd ~/Documents"
alias dt="cd ~/Desktop"
alias lambda="cd ~/Documents/Lambda-School"
alias python='python3'
alias pip='pip3'
